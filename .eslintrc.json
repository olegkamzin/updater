{
    "env": {
        "browser": true,
        "es2022": true,
        "node": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-tabs": "off",
        "no-mixed-spaces-and-tabs": "error",
        "indent": ["error", "tab"],
        "camelcase": "off",
        "no-unused-vars": "warn"
    }
}
